<TLVList>
	<TLVMsg name="FUNCTIONAL_SPEC" type="0x800001" length="-1" description="(SMARTPHONE/OBU/ALIX -> LAPTOP)">
		<TLV name="DEVICE_ID"/>
		<TLV name="DEVICE_TYPE"/>
		<TLV name="SENSOR_SPEC_LIST"/>
		<TLV name="COMM_SPEC_LIST"/>
		<TLV name="APP_SPEC_LIST"/>
	</TLVMsg>
	
	<TLVMsg name="CONFIGURATION_SPEC" type="0x800002" length="-1" description="(LAPTOP -> SMARTPHONE/OBU/ALIX)">
		<TLV name="SENSOR_SPEC_LIST"/>
		<TLV name="COMM_SPEC_LIST"/>
		<TLV name="APP_SPEC_LIST"/>
	</TLVMsg>
	
	<TLVMsg name="SENSOR_INFO" type="0x800011" length="-1" description="(SMARTPHONE/OBU/ALIX -> LAPTOP)">
		<TLV name="DEVICE_ID"/>
		<TLV name="SENSOR_VALUE_LIST"/>
	</TLVMsg>
	
	<TLVMsg name="SENSOR_INFO_ACK" type="0x800012" length="0x000007" description="(LAPTOP -> SMARTPHONE/OBU/ALIX)">
		<TLV name="RESPONSE_CODE"/>
	</TLVMsg>
	
	<TLVMsg name="VEHICLE_INFO" type="0x800021" length="0x000026" description="(LAPTOP -&gt; OBU/ALIX)">
		<TLV name="SELF_MOBILITY"/>
	</TLVMsg>
	
	<TLVMsg name="VEHICLE_INFO_ACK" type="0x800022" length="0x000007" description="(OBU/ALIX -&gt; LAPTOP)">
		<TLV name="RESPONSE_CODE"/>
	</TLVMsg>
	
	<TLVMsg name="REMOTE_INFO" type="0x800031" length="-1" description="(OBU/ALIX -&gt; LAPTOP)">
		<TLV name="RSU_LIST"/>
		<TLV name="NEIGHBOR_LIST"/>
		<TLV name="PEDESTRIAN_LIST"/>
	</TLVMsg>
	
	<TLVMsg name="REMOTE_INFO_ACK" type="0x800032" length="0x000007" description="(LAPTOP -&gt; OBU/ALIX)">
		<TLV name="RESPONSE_CODE"/>
	</TLVMsg>
	
	<TLVMsg name="MOBILITY_SUMMARY" type="0x800041" length="-1" description="(LAPTOP -&gt; SMARTPHONE)">
		<TLV name="SELF_MOBILITY"/>
		<TLV name="RSU_LIST"/>
		<TLV name="NEIGHBOR_LIST"/>
		<TLV name="PEDESTRIAN_LIST"/>
		<TLV name="OBJECT_LIST"/>
	</TLVMsg>
	
	<TLVMsg name="MOBILITY_SUMMARY_ACK" type="0x800042" length="0x000007" description="(SMARTPHONE -&gt; LAPTOP)">
		<TLV name="RESPONSE_CODE"/>
	</TLVMsg>
	

	<TLV name="SENSOR_SPEC_LIST" type="0x400001" length="-1" description="">
		<TLV name="SENSOR_SPEC" repeat="true"/>
	</TLV>
	
	<TLV name="COMM_SPEC_LIST" type="0x400002" length="-1" description="">
		<TLV name="COMM_SPEC" repeat="true"/>
	</TLV>
	
	<TLV name="APP_SPEC_LIST" type="0x400003" length="-1" description="">
		<TLV name="APP_SPEC" repeat="true"/>
	</TLV>

	<TLV name="SENSOR_VALUE_LIST" type="0x400004" length="-1" description="">
		<TLV name="GPS_SAMPLE" repeat="true"/>
	</TLV>

	<TLV name="SELF_MOBILITY" type="0x400006" length="0x000020" description="">
		<TLV name="GPS_SAMPLE"/>
	</TLV>

	<TLV name="RSU_LIST" type="0x400007" length="-1" description="">
		<TLV name="RSU_INFO" repeat="true"/>
	</TLV>

	<TLV name="NEIGHBOR_LIST" type="0x400008" length="-1" description="">
		<TLV name="NEIGHBOR_INFO" repeat="true"/>
	</TLV>

	<TLV name="PEDESTRIAN_LIST" type="0x400009" length="-1" description="">
		<TLV name="PEDESTRIAN_INFO" repeat="true"/>
	</TLV>

	<TLV name="NEIGHBOR_INFO" type="0x40000A" length="0x00002E" description="">
		<TLV name="VEHICLE_ID"/>
		<TLV name="GPS_SAMPLE"/>
	</TLV>

	<TLV name="PEDESTRIAN_INFO" type="0x40000B" length="0x00002E" description="">
		<TLV name="PEDESTRIAN_ID"/>
		<TLV name="GPS_SAMPLE"/>
	</TLV>
	
	<TLV name="OBJECT_LIST" type="0x40000C" length="-1" description="">
		<TLV name="OBJECT_INFO" repeat="true"/>
	</TLV>
	
	<TLV name="OBJECT_INFO" type="0x40000D" length="0x00002E" description="">
		<TLV name="OBJECT_ID"/>
		<TLV name="GPS_SAMPLE"/>
		<TLV name="DIMENSION"/>
	</TLV>
	
	
	<TLV name="DEVICE_ID" type="0x000001" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="DEVICE_TYPE" type="0x000002" length="0x000001" description="">
		<value type="uint8_t"></value>
	</TLV>
	
	<TLV name="SENSOR_SPEC" type="0x000003" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="COMM_SPEC" type="0x000004" length="0x000004" description="">
		<value type="uint32_t"></value>
	</TLV>
	
	<TLV name="APP_SPEC" type="0x000005" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="RSU_INFO" type="0x000006" length="0x000010" description="">
		<value type="uint128_t"></value>
	</TLV>
	
	<TLV name="VEHICLE_ID" type="0x000007" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="PEDESTRIAN_ID" type="0x000008" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="GPS_SAMPLE" type="0x000009" length="0x00001A" description="">
		<value type="uint208_t"></value>
	</TLV>
	
	<TLV name="RESPONSE_CODE" type="0x00000A" length="0x000001" description="">
		<value type="uint8_t"></value>
	</TLV>
	
	<TLV name="OBJECT_ID" type="0x00000B" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
	
	<TLV name="DIMENSION" type="0x00000C" length="0x000008" description="">
		<value type="uint64_t"></value>
	</TLV>
</TLVList>
